import sys, gc
gc.disable()

def create_coroutine(n):
    if n > 1:
	cn = create_coroutine(n-1).next
	while 1:
	    yield cn()+1
    while 1:
	yield 1

cn = create_coroutine(500).next
print sum(cn() for i in xrange(int(sys.argv[1])))
